    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        },
        {
            "name": "2022 (console)",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}/2022/bin/Debug/net7.0/2022.dll",
            "args": [
                "5"
            ],
            "cwd": "${workspaceFolder}/2022",
            "stopAtEntry": false,
            "console": "internalConsole"
        }
    ]
}